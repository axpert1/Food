<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.anilxpert.food">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <application
        android:name=".AppInitialization"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.Splace"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.AppBgWhite">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.SelectionActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.DashBordActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".activity.LogRegActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".activity.OrderSelectActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".activity.OrderSummryActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".activity.MyOrderActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".activity.FaqActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".activity.FindActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".activity.PolicyActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".activity.ProfileActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".activity.TermsActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".activity.ContectUsActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".activity.CuponCodeActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".activity.PaymentOrder"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".activity.ScaneCode"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".activity.OrderDetails"
            android:screenOrientation="portrait"></activity>

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/app_id" />

        <activity
            android:name="com.facebook.FacebookActivity"
            android:label="@string/app_name" />
    </application>

</manifest>